class Solution {
    public boolean isIsomorphic(String s, String t) {
        if(s.length()!=t.length()) return false;

        int [] s1map = new int [256];
        int [] s2map = new int [256];

        for(int i =0;i<s.length();i++){
            char c =s.charAt(i);
            char c2 = t.charAt(i);

            if(s1map[c]!=s2map[c2]){
                return false;
            }

             s1map[c]=i+1;
            s2map[c2]=i+1;
        }
        return true;
    }
}
